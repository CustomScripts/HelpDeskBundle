{
    "name": "customscripts/helpdesk-bundle",
    "description": "Adds HelpDesk functionality to a Symfony application",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": [
        "help-desk",
        "helpdesk",
        "helpdesk-bundle",
        "helpdesk bundle",
        "symfony bundle"
    ],
    "authors": [
        {
            "name": "Pierre du Plessis",
            "email": "info@customscripts.co.za"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php" : ">=5.4.0",
        "symfony/framework-bundle": "~2.3",
        "symfony/form": "~2.3"
    },
    "autoload" : {
        "psr-4" : {
            "CS\\HelpDeskBundle\\" : "src"
        }
    },
    "require-dev": {
        "phpspec/phpspec": "~2.2"
    },
    "config": {
        "bin-dir": "bin"
    }
}
